#include "74HC595.h"
HAL_StatusTypeDef Send_Data(HC595* _hc595,uint8_t dt)
{
	for (int c = 0;c<8;c++)
	{
		HAL_GPIO_WritePin(_hc595->GPIO_CLK,_hc595->GPIO_CLK_PIN, GPIO_PIN_SET);
		HAL_GPIO_WritePin(_hc595->GPIO_DATA, _hc595->GPIO_DATA_PIN, (dt &(1 << c))>>c);
	}
}
