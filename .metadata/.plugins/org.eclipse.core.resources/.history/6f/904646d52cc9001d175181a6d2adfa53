#ifndef INC_74HC595_H_
#define INC_74HC595_H_

#include "stm32f1xx.h"
typedef struct{
	GPIO_TypeDef* OE;
	GPIO_TypeDef* CLK;
	GPIO_TypeDef* DATA;
	GPIO_TypeDef* CLOCK;
} HC595;
HAL_StatusTypeDef Send_Data(HC595* uint8_t dt);
#endif /* INC_74HC595_H_ */
