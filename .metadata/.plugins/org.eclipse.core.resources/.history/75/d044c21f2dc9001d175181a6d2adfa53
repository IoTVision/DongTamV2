#ifndef INC_74HC595_H_
#define INC_74HC595_H_

#include "stm32f1xx.h"
typedef struct{
	GPIO_TypeDef* OE;
	uint16_t GPIO_CLK_PIN;
	GPIO_TypeDef* GPIO_CLK;
	uint16_t GPIO_CLK_PIN;
	GPIO_TypeDef* GPIO_DATA;
	uint16_t GPIO_CLK_PIN;
	GPIO_TypeDef* GPIO_CLOCK;
	uint16_t GPIO_CLK_PIN;
} HC595;
HAL_StatusTypeDef Send_Data(HC595* _hc595,uint8_t dt);
#endif /* INC_74HC595_H_ */
